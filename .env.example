# LiteLLM Master Key - used to authenticate API requests
LITELLM_MASTER_KEY=sk-litellm-master-key-change-me
# LiteLLM Salt Key - used to encrypt/decrypt API credentials
# Do not change after initial setup
LITELLM_SALT_KEY=sk-litellm-salt-key-change-me
# Secret key for Open WebUI
WEBUI_SECRET_KEY=open-webui-secret-key-change-me
# API Keys for various LLM providers
ANTHROPIC_API_KEY=sk-
OPENAI_API_KEY=sk-
# PostgreSQL configuration
POSTGRES_USER=litellm
POSTGRES_PASSWORD=secure_password_to_change
POSTGRES_DB=litellm
